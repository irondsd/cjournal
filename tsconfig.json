{
  "compilerOptions": {
  "allowJs": true,
  "allowSyntheticDefaultImports": true,
  "esModuleInterop": true,
  "isolatedModules": true,
  "jsx": "react",
  "lib": ["es6", "dom"],
  "moduleResolution": "node",
  "noEmit": true,
  "strict": true,
  "target": "esnext"
  },
  "baseUrl": ".",
  "paths": {
    "@/*": [
      "src/*"
    ]
  },
  "exclude": [
    "node_modules", 
    "android", 
    "ios",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
}
