{
    "name": "cjournal",
    "version": "4.1.0",
    "private": true,
    "scripts": {
        "start": "react-native start",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "release:a": "react-native run-android --variant=release",
        "release:i": "react-native run-ios --configuration Release --device \"iPhone (Konstantin)\"",
        "build:a": "react-native bundle --platform android --dev false --entry-file index.tsx --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && rm -r android/app/src/main/res/drawable* android/app/src/main/res/raw && cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets && cd ..",
        "build:play": "cd android && ./gradlew bundleRelease",
        "build:ios": "react-native bundle --entry-file='index.tsx' --bundle-output='./ios/cjournal/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
        "pods": "cd ios && pod install && cd ..",
        "clean": "cd android && ./gradlew clean && cd .."
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.6.3",
        "@react-native-community/datetimepicker": "^2.4.0",
        "@react-native-community/geolocation": "^2.0.2",
        "@react-native-community/netinfo": "^5.0.0",
        "@t2tx/react-native-universal-pedometer": "^2.0.3",
        "react": "16.9.0",
        "react-native": "^0.62.2",
        "react-native-app-auth": "^5.1.1",
        "react-native-audio-record": "^0.2.2",
        "react-native-background-fetch": "^2.7.1",
        "react-native-background-timer": "^2.1.1",
        "react-native-base64": "0.0.2",
        "react-native-camera-kit": "^8.0.4",
        "react-native-circular-progress": "^1.3.6",
        "react-native-fs": "^2.16.2",
        "react-native-gesture-handler": "^1.5.2",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-localization": "^2.1.6",
        "react-native-modal-datetime-picker": "^8.7.1",
        "react-native-push-notification": "^3.1.9",
        "react-native-qrcode-svg": "^6.1.1",
        "react-native-reanimated": "^1.4.0",
        "react-native-root-siblings": "^4.0.6",
        "react-native-root-toast": "^3.2.1",
        "react-native-screens": "^1.0.0-alpha.23",
        "react-native-sensors": "^5.3.3",
        "react-native-sound": "^0.11.0",
        "react-native-svg": "^12.1.0",
        "react-native-svg-transformer": "^0.14.3",
        "react-native-vector-icons": "^6.6.0",
        "react-native-webview": "10.1.1",
        "react-navigation": "^4.0.10",
        "react-navigation-stack": "^1.10.3",
        "react-navigation-tabs": "^2.6.2",
        "react-redux": "^7.1.3",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/runtime": "^7.13.10",
        "@react-native-community/eslint-config": "^0.0.5",
        "@types/react": "^17.0.3",
        "@types/react-native": "^0.63.51",
        "@types/react-native-base64": "^0.1.0",
        "@types/react-native-push-notification": "^7.0.1",
        "@types/react-native-vector-icons": "^6.4.6",
        "@types/react-redux": "^7.1.16",
        "babel-jest": "^24.9.0",
        "babel-plugin-root-import": "^6.6.0",
        "eslint": "^6.7.2",
        "jest": "^24.9.0",
        "jetifier": "^1.6.5",
        "metro-react-native-babel-preset": "^0.57.0",
        "react-test-renderer": "16.9.0"
    },
    "jest": {
        "preset": "react-native"
    }
}
