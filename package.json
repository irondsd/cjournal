{
    "name": "cjournal",
    "version": "3.7.5",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios --device \"iPhone\"",
        "start": "react-native start",
        "test": "jest",
        "build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && rm -r android/app/src/main/res/drawable* android/app/src/main/res/raw && cd android && ./gradlew assembleRelease -x bundleReleaseJsAndAssets && cd ..",
        "pods": "cd ios && pod install && cd ..",
        "android-r": "react-native run-android --variant=release",
        "ios-r": "react-native run-ios --configuration Release --device \"iPhone (Konstantin)\"",
        "build-play": "cd android && ./gradlew bundleRelease"
    },
    "dependencies": {
        "@JWWon/react-native-universal-pedometer": "git+https://github.com/stephenheron/react-native-universal-pedometer.git",
        "@react-native-community/async-storage": "^1.6.3",
        "@react-native-community/geolocation": "^2.0.2",
        "@react-native-community/netinfo": "^5.0.0",
        "react": "16.9.0",
        "react-native": "^0.62.2",
        "react-native-app-auth": "^5.1.1",
        "react-native-audio-record": "^0.2.2",
        "react-native-background-fetch": "^2.7.1",
        "react-native-background-timer": "^2.1.1",
        "react-native-base64": "0.0.2",
        "react-native-camera-kit": "^8.0.4",
        "react-native-circular-progress": "^1.3.6",
        "react-native-fs": "^2.16.2",
        "react-native-gesture-handler": "^1.5.2",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-localization": "^2.1.6",
        "react-native-modal-datetime-picker": "^7.6.0",
        "react-native-push-notification": "^3.1.9",
        "react-native-reanimated": "^1.4.0",
        "react-native-root-toast": "^3.2.0",
        "react-native-screens": "^1.0.0-alpha.23",
        "react-native-sensors": "^5.3.3",
        "react-native-sound": "^0.11.0",
        "react-native-svg": "^12.0.3",
        "react-native-vector-icons": "^6.6.0",
        "react-navigation": "^4.0.10",
        "react-navigation-stack": "^1.10.3",
        "react-navigation-tabs": "^2.6.2",
        "react-redux": "^7.1.3",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.7.5",
        "@babel/runtime": "^7.7.6",
        "@react-native-community/eslint-config": "^0.0.5",
        "babel-jest": "^24.9.0",
        "eslint": "^6.7.2",
        "jest": "^24.9.0",
        "jetifier": "^1.6.5",
        "metro-react-native-babel-preset": "^0.57.0",
        "react-test-renderer": "16.9.0"
    },
    "jest": {
        "preset": "react-native"
    }
}
